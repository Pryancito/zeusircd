msgid "My name is: %s"
msgstr "Mi nombre es: %s"

msgid "Zeus IRC Daemon started"
msgstr "Demonio Zeus iRCd iniciado"

msgid "You have started wrong the ircd. For help check: %s -h"
msgstr "Has iniciado mal el ircd. Para ayuda consulta: %s -h"

msgid "Use: %s [-c server.conf] [-p password] -f [-start|-stop|-restart]"
msgstr "Uso: %s [-c server.conf] [-p password] -f [-start|-stop|-restart]"

msgid "Start: %s -start | Stop: %s -stop | Restart: %s -restart"
msgstr "Iniciar: %s -start | Parar: %s -stop | Reiniciar: %s -restart"

msgid "Error loading config file."
msgstr "Error al cargar el archivo de configuracion."

msgid "Password %s crypted: %s"
msgstr "Password %s encriptada: %s"

msgid "The server is already started, if not, delete zeus.pid file."
msgstr "El servidor ya esta iniciado, si no es asi borre el archivo zeus.pid."

msgid "The server is not started, if not, stop it manually."
msgstr "El servidor no esta iniciado, si no es asi parelo manualmente."

msgid "User limit set to: %s"
msgstr "Limite de usuarios configurado a: %s"

msgid "Data Error."
msgstr "Error en los datos."

msgid "Error in nick or channel input."
msgstr "Error al introducir el nick o el canal."

msgid "The channel contains no-valid characters."
msgstr "El canal contiene caracteres no validos."

msgid "The channel %s is already registered."
msgstr "El canal %s ya esta registrado."

msgid "The channel %s is not registered."
msgstr "El canal %s no esta registrado."

msgid "The nick contains no-valid characters."
msgstr "El nick contiene caracteres no validos."

msgid "The nick %s is already registered."
msgstr "El nick %s ya esta registrado."

msgid "The nick %s is not registered."
msgstr "El nick %s no esta registrado."

msgid "Error at data input."
msgstr "Error al introducir los datos."

msgid "The HUB doesnt exists, DBs are in read-only mode."
msgstr "El HUB no existe, las BDs estan en modo de solo lectura."

msgid "The channel %s cannot be registered. Please contact with an iRCop."
msgstr "El canal %s no ha sido registrado. Contacte con algun iRCop."

msgid "The channel has been registered."
msgstr "El canal ha sido registrado."

msgid "The channel %s has been registered."
msgstr "El canal %s ha sido registrado."

msgid "The password contains no valid characters (!:;')."
msgstr "El password contiene caracteres no validos (!:;')."
